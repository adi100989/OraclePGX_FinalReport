/****************************************************** 
*   Code to test if maps are iterable	
*******************************************************/

procedure MapLabelKey(g:graph,ID:N_P <int>(g) ):int
{
	int i=1;
	if( g.numNodes()<=1 ) return 0;
	map<int,int> sample;
	
	foreach(n:g.nodes)
        {
		if(sample.hasKey(n.ID))
			sample[n.ID]+=1;
		else
			sample[n.ID]=1;	
	}
	int cnt;
	for(item:sample.items)
	{
		cnt += item;
	}	

	return cnt;
}

